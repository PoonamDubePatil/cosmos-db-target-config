{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "$schema": {
      "$id": "/properties/$schema",
      "type": "string",
      "title": "The $schema Schema ",
      "default": "",
      "examples": [
        "TODO"
      ]
    },
    "destructiveFlags": {
      "$id": "/properties/destructiveFlags",
      "type": "array",
      "items": {
        "$id": "/properties/destructiveFlags/items",
        "type": "string",
        "title": "The 0th Schema ",
        "default": "",
        "examples": [
          "database",
          "collection",
          "storedProcedure"
        ]
      }
    },
    "databases": {
      "$id": "/properties/databases",
      "type": "array",
      "items": {
        "$id": "/properties/databases/items",
        "type": "object",
        "properties": {
          "name": {
            "$id": "/properties/databases/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "throughput-in-collections"
            ]
          },
          "collections": {
            "$id": "/properties/databases/items/properties/collections",
            "type": "array",
            "items": {
              "$id": "/properties/databases/items/properties/collections/items",
              "type": "object",
              "properties": {
                "name": {
                  "$id": "/properties/databases/items/properties/collections/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema ",
                  "default": "",
                  "examples": [
                    "partitioned"
                  ]
                },
                "partitionKey": {
                  "$id": "/properties/databases/items/properties/collections/items/properties/partitionKey",
                  "type": "string",
                  "title": "The Partitionkey Schema ",
                  "default": "",
                  "examples": [
                    "/dept"
                  ]
                },
                "requestUnits": {
                  "$id": "/properties/databases/items/properties/collections/items/properties/requestUnits",
                  "type": "integer",
                  "title": "The Requestunits Schema ",
                  "default": 0,
                  "examples": [
                    1200
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
